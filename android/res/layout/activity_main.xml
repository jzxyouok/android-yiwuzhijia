<com.android.house.view.DragLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/dl"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff" >

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" >
		
        <RelativeLayout
            android:id="@+id/user_info"
            android:layout_width="fill_parent"
            android:layout_height="150dp"
            android:background="@drawable/user_info_background" >

            <TextView
                android:id="@+id/user_get_money"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/user_money"
                android:layout_alignBottom="@+id/user_money"
                android:layout_marginBottom="16dp"
                android:layout_marginLeft="16dp"
                android:layout_toRightOf="@+id/user_money"
                android:background="@drawable/text_shape_gray_ovel_rec"
                android:gravity="bottom|center_horizontal"
                android:paddingLeft="4dp"
                android:paddingRight="4dp"
                android:text="提现"
                android:textColor="#FF3000"
                
                android:textSize="16sp" />

            <com.BeeFramework.view.WebImageView
                android:id="@+id/user_img"
                android:layout_width="100dp"
                android:layout_alignParentBottom="true"
                android:layout_height="100dp"
                android:layout_alignParentLeft="true"
                android:layout_marginBottom="20dp"
                android:layout_marginLeft="32dp"
                
                android:src="@drawable/icon_default_user_img" />

            <TextView
                android:id="@+id/user_money"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBottom="@+id/user_img"
                android:layout_alignLeft="@+id/user_name"
                android:layout_weight="1.0"
                android:background="@drawable/text_shape_gray_ovel_rec"
                android:gravity="center"
                android:text="0.0"
                android:textColor="#ffffff"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/user_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_above="@+id/user_money"
                android:layout_marginBottom="8dp"
                android:layout_marginLeft="16dp"
                android:layout_toRightOf="@+id/user_img"
                android:layout_weight="1.0"
                android:gravity="bottom"
                android:text="客户姓名"
                android:textColor="#ffffff"
                android:textSize="16sp" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/user_func"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/user_info" >

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >

                <LinearLayout
                    android:id="@+id/my_house"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="4dp"
                    android:layout_marginLeft="8dp"
                    android:layout_marginRight="4dp"
                    android:layout_marginTop="16dp"
                    android:orientation="vertical" >

                    <ImageView
                        android:id="@+id/my_house_img"
                        android:layout_width="48dp"
                        android:layout_height="48dp"
                        android:layout_gravity="center"
                        android:src="@drawable/icon_my_house" />

                    <TextView
                        android:id="@+id/my_house_text"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginTop="16dp"
                        android:text="我的楼盘"
                        android:textColor="#6B6B6B"
                        android:textSize="16sp" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/my_wallet"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="4dp"
                    android:layout_marginLeft="8dp"
                    android:layout_marginRight="4dp"
                    android:layout_marginTop="16dp"
                    android:orientation="vertical" >

                    <ImageView
                        android:id="@+id/my_wallet_img"
                        android:layout_width="48dp"
                        android:layout_height="48dp"
                        android:layout_gravity="center"
                        android:src="@drawable/icon_my_wallet" />

                    <TextView
                        android:id="@+id/my_wallet_text"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginTop="16dp"
                        android:text="我的钱包"
                        android:textColor="#6B6B6B"
                        android:textSize="16sp" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/my_manager"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="4dp"
                    android:layout_marginLeft="8dp"
                    android:layout_marginRight="4dp"
                    android:layout_marginTop="16dp"
                    android:orientation="vertical" >

                    <ImageView
                        android:id="@+id/my_manager_img"
                        android:layout_width="48dp"
                        android:layout_height="48dp"
                        android:layout_gravity="center"
                        android:src="@drawable/icon_my_manager" />

                    <TextView
                        android:id="@+id/my_manager_text"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginTop="16dp"
                        android:text="我的经纪人"
                        android:textColor="#6B6B6B"
                        android:textSize="16sp" />
                </LinearLayout>
            </LinearLayout>
        </RelativeLayout>

        <LinearLayout
            android:id="@+id/extra_func"
            android:layout_width="232dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/user_func"
            android:layout_marginTop="8dp"
            android:orientation="vertical" >

            <View
                android:layout_width="fill_parent"
                android:layout_height="0.1dp"
                android:background="#E9E9E9" />

            <RelativeLayout
                android:id="@+id/change_psd"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:background="#ffffff"
                android:paddingBottom="8dp"
                android:paddingLeft="16dp"
                android:paddingRight="8dp"
                android:paddingTop="8dp" >

                <ImageView
                    android:id="@+id/change_psd_img"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_alignParentLeft="true"
                    android:src="@drawable/icon_change_psd" />

                <TextView
                    android:id="@+id/change_psd_text"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="16dp"
                    android:layout_toLeftOf="@+id/change_psd_arrow"
                    android:layout_toRightOf="@+id/change_psd_img"
                    android:gravity="center_vertical"
                    android:text="修改密码"
                    android:textColor="#6F6F6F"
                    android:textSize="16sp" />

                <ImageView
                    android:id="@+id/change_psd_arrow"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentTop="true"
                    android:src="@drawable/icon_arrow_right_btn" />
            </RelativeLayout>

            <View
                android:layout_width="fill_parent"
                android:layout_height="0.1dp"
                android:background="#E9E9E9" />

            <RelativeLayout
                android:id="@+id/share"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:background="#ffffff"
                android:paddingBottom="8dp"
                android:paddingLeft="16dp"
                android:paddingRight="8dp"
                android:paddingTop="8dp" >

                <ImageView
                    android:id="@+id/share_img"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_alignParentLeft="true"
                    android:src="@drawable/icon_share" />

                <TextView
                    android:id="@+id/share_text"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="16dp"
                    android:layout_toLeftOf="@+id/share_arrow"
                    android:layout_toRightOf="@+id/share_img"
                    android:gravity="center_vertical"
                    android:text="告诉朋友"
                    android:textColor="#6F6F6F"
                    android:textSize="16sp" />

                <ImageView
                    android:id="@+id/share_arrow"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentTop="true"
                    android:src="@drawable/icon_arrow_right_btn" />
            </RelativeLayout>

            <View
                android:layout_width="fill_parent"
                android:layout_height="0.1dp"
                android:background="#E9E9E9" />

            <RelativeLayout
                android:id="@+id/feedback"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:background="#ffffff"
                android:paddingBottom="8dp"
                android:paddingLeft="16dp"
                android:paddingRight="8dp"
                android:paddingTop="8dp" >

                <ImageView
                    android:id="@+id/feedback_img"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_alignParentLeft="true"
                    android:src="@drawable/icon_feedback" />

                <TextView
                    android:id="@+id/feedback_text"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="16dp"
                    android:layout_toLeftOf="@+id/feedback_arrow"
                    android:layout_toRightOf="@+id/feedback_img"
                    android:gravity="center_vertical"
                    android:text="建议反馈"
                    android:textColor="#6F6F6F"
                    android:textSize="16sp" />

                <ImageView
                    android:id="@+id/feedback_arrow"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentTop="true"
                    android:src="@drawable/icon_arrow_right_btn" />
            </RelativeLayout>

            <View
                android:layout_width="fill_parent"
                android:layout_height="0.1dp"
                android:background="#E9E9E9" />

            <RelativeLayout
                android:id="@+id/about"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:background="#ffffff"
                android:paddingBottom="8dp"
                android:paddingLeft="16dp"
                android:paddingRight="8dp"
                android:paddingTop="8dp" >

                <ImageView
                    android:id="@+id/about_img"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_alignParentLeft="true"
                    android:src="@drawable/icon_about" />

                <TextView
                    android:id="@+id/about_text"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="16dp"
                    android:layout_toLeftOf="@+id/about_arrow"
                    android:layout_toRightOf="@+id/about_img"
                    android:gravity="center_vertical"
                    android:text="关于易屋之家"
                    android:textColor="#6F6F6F"
                    android:textSize="16sp" />

                <ImageView
                    android:id="@+id/about_arrow"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentTop="true"
                    android:src="@drawable/icon_arrow_right_btn" />
            </RelativeLayout>

            <View
                android:layout_width="fill_parent"
                android:layout_height="0.1dp"
                android:background="#E9E9E9" />

            <LinearLayout
                android:id="@+id/login_out"
                android:layout_width="232dp"
                android:layout_height="wrap_content"
                android:background="#ECECEC"
                android:orientation="vertical" >

                <RelativeLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:background="#ffffff" >

                    <TextView
                        android:id="@+id/login_out_text"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="12dp"
                        android:background="#ffffff"
                        android:gravity="left|center_vertical"
                        android:paddingBottom="8dp"
                        android:paddingLeft="80dp"
                        android:text="退出登录"
                        android:textColor="#6F6F6F"
                        android:textSize="16sp" />

                    <TextView
                        android:id="@+id/login_out_kefu_phone"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentBottom="true"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="32dp"
                        android:background="#ffffff"
                        android:paddingBottom="8dp"
                        android:text="客服电话：400000818"
                        android:textColor="#6F6F6F"
                        android:textSize="10sp" />

                    <TextView
                        android:id="@+id/login_out_weixin"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentBottom="true"
                        android:layout_alignParentLeft="true"
                        android:layout_marginLeft="8dp"
                        android:background="#ffffff"
                        android:paddingBottom="8dp"
                        android:text="微信账号：易屋之家"
                        android:textColor="#6F6F6F"
                        android:textSize="10sp" />
                </RelativeLayout>
            </LinearLayout>
        </LinearLayout>
    </RelativeLayout>

    <com.android.house.view.MyRelativeLayout
        android:id="@+id/map_page"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#eeeeee" 
        
   
        >
      

        <!-- android:visibility="gone" -->

        <RelativeLayout
            android:id="@+id/house_main_title"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:background="#ffffff"
             >

            <ImageView
                android:id="@+id/profile_icon"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_centerVertical="true"
                android:layout_marginLeft="16dp"
                android:scaleType="centerCrop"
                android:src="@drawable/icon_profile" />

            <ImageView
                android:id="@+id/house_main_menu"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:padding="5dp"
                android:layout_alignParentRight="true"
                android:layout_alignTop="@+id/profile_icon"
                android:layout_centerVertical="true"
                android:layout_marginRight="16dp"
                android:src="@drawable/icon_menu" />

            <LinearLayout
                android:id="@+id/linearLayout1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:layout_marginBottom="8dp"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:text="易屋"
                    android:textColor="#D04509"
                    android:textSize="18sp" />

                <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="8dp"
                    android:layout_marginRight="8dp"
                    android:src="@drawable/ic_launcher" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:text="之家"
                    android:textColor="#D04509"
                    android:textSize="18sp" />
            </LinearLayout>
        </RelativeLayout>

        <LinearLayout
            android:id="@+id/house_main_filter"
            android:layout_width="fill_parent"
            android:layout_height="36dp"
            android:layout_below="@+id/house_main_title"
            android:background="#eeeeee"
             android:visibility="gone" 
           >

            <View
                android:layout_width="0.8dp"
                android:layout_height="fill_parent"
                android:layout_marginBottom="8dp"
                android:layout_marginTop="8dp"
                android:background="#dcdcdc" />

            <TextView
                android:id="@+id/filter_price"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="1.0"
                android:gravity="center"
                android:text="价格" />

            <View
                android:layout_width="0.8dp"
                android:layout_height="fill_parent"
                android:layout_marginBottom="8dp"
                android:layout_marginTop="8dp"
                android:background="#dcdcdc" />

            <TextView
                android:id="@+id/filter_location"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="1.0"
                android:gravity="center"
                android:text="位置" />

            <View
                android:layout_width="0.8dp"
                android:layout_height="fill_parent"
                android:layout_marginBottom="8dp"
                android:layout_marginTop="8dp"
                android:background="#dcdcdc" />

            <TextView
                android:id="@+id/filter_type"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="1.0"
                android:gravity="center"
                android:text="户型" />

            <View
                android:layout_width="0.8dp"
                android:layout_height="fill_parent"
                android:layout_marginBottom="8dp"
                android:layout_marginTop="8dp"
                android:background="#dcdcdc" />

            <TextView
                android:id="@+id/filter_decoration"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="1.0"
                android:gravity="center"
                android:text="装修" />

            <View
                android:layout_width="0.8dp"
                android:layout_height="fill_parent"
                android:layout_marginBottom="4dp"
                android:layout_marginTop="4dp"
                android:background="#dcdcdc" />
        </LinearLayout>

        <com.baidu.mapapi.map.MapView
            android:background="#ffffff"
            android:id="@+id/house_main_mapview"
            android:layout_width="match_parent"
            android:layout_height="fill_parent"
            android:layout_above="@+id/main_tab"
            android:layout_below="@+id/house_main_filter" />
		
        <LinearLayout
            android:id="@+id/main_tab"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:background="#ffffff"
            android:orientation="vertical" >

            <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="8dp"
                android:layout_marginTop="8dp" >

                <LinearLayout
                    android:id="@+id/house_main_new_house_wrapper"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:layout_marginLeft="8dp"
                    android:layout_marginRight="8dp"
                    android:orientation="vertical" >

                    <ImageView
                        android:id="@+id/house_main_new_house_img"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:src="@drawable/icon_small_red" />

                    <TextView
                        android:id="@+id/house_main_new_house"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="8dp"
                        android:layout_weight="1.0"
                        android:gravity="center"
                        android:text="新楼盘"
                        android:textColor="#585858"
                        android:textSize="16sp" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/house_main_manager_wrapper"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentTop="true"
                    android:layout_marginRight="8dp"
                    android:layout_toLeftOf="@+id/house_main_new_house_wrapper"
                    android:orientation="vertical" >

                    <ImageView
                        android:id="@+id/house_main_manager_img"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:src="@drawable/icon_small_gray" />

                    <TextView
                        android:id="@+id/house_main_manager"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="8dp"
                        android:layout_weight="1.0"
                        android:gravity="center"
                        android:text="经纪人"
                        android:textColor="#585858"
                        android:textSize="16sp" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/house_main_free_car_wrapper"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentTop="true"
                    android:layout_marginLeft="8dp"
                    android:layout_toRightOf="@+id/house_main_new_house_wrapper"
                    android:orientation="vertical" >

                    <ImageView
                        android:id="@+id/house_main_free_car_img"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:src="@drawable/icon_small_gray" />

                    <TextView
                        android:id="@+id/house_main_free_car"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="8dp"
                        android:layout_weight="1.0"
                        android:gravity="center"
                        android:text="免费专车"
                        android:textColor="#585858"
                        android:textSize="16sp" />
                </LinearLayout>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/house_main_search_wrapper"
                android:layout_width="wrap_content"
                android:layout_height="32dp"
                android:layout_marginBottom="16dp"
                android:layout_marginLeft="16dp"
                android:layout_marginRight="16dp"
                android:layout_marginTop="8dp"
                android:background="@drawable/edit_main_search_shape" >

                <EditText
                    android:id="@+id/house_main_search"
                    android:layout_width="wrap_content"
                    android:layout_height="30dp"
                    android:layout_alignParentLeft="true"
                    android:layout_marginLeft="16dp"
                    android:layout_toLeftOf="@+id/house_main_search_img"
                    android:background="#ffffff"
                    android:ellipsize="end"
                    android:ems="10"
                    android:gravity="left|center_vertical"
                    android:hint="请输入楼盘名称或者经纪人名"
                    android:singleLine="true"
                    android:textSize="14sp" >
                </EditText>

                <ImageView
                    android:id="@+id/house_main_search_img"
                    android:layout_width="32dp"
                    android:layout_height="32dp"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:src="@drawable/icon_search_btn" />
            </RelativeLayout>
        </LinearLayout>
        
        
       
        </com.android.house.view.MyRelativeLayout>



</com.android.house.view.DragLayout>