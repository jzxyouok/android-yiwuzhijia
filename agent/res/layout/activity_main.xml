<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff" >

    <RelativeLayout
        android:id="@+id/manager_info"
        android:layout_width="fill_parent"
        android:layout_height="192dp"
        android:background="@drawable/user_info_background" >

        <TextView
            android:id="@+id/home_manager_id"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/home_manager_id_text"
            android:layout_alignBottom="@+id/home_manager_id_text"
            android:layout_toRightOf="@+id/home_manager_id_text"
            android:text=""
            android:textColor="#EFF6FF"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/home_manager_money"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/home_manager_id"
            android:layout_alignBottom="@+id/home_manager_id"
            android:layout_toRightOf="@+id/home_manager_id"
            android:background="#66ffffff"
            android:layout_marginLeft="8dp"
            android:padding="4dp"
            android:text="佣金管理"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/home_manager_id_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/home_manager_give_money_text"
            android:layout_marginLeft="32dp"
            android:layout_marginBottom="16dp"
            android:text="ID:"
            android:textColor="#EFF6FF"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/home_manager_get_money_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/home_manager_id_text"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="32dp"
            android:text="发放佣金："
            android:textColor="#ffffff"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/home_logo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/home_manager_money"
            android:layout_alignLeft="@+id/home_manager_id_text"
            android:layout_marginBottom="32dp"
            android:text="易屋之家"
            android:textSize="22sp"
            android:textColor="#838383" />

        <TextView
            android:id="@+id/home_manager_get_money"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/home_manager_get_money_text"
            android:layout_alignBottom="@+id/home_manager_get_money_text"
            android:layout_toRightOf="@+id/home_manager_get_money_text"
            android:layout_marginRight="16dp"
            android:text=""
            android:textColor="#ffffff"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/home_manager_give_money_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/home_manager_get_money"
            android:layout_alignBottom="@+id/home_manager_get_money"
            android:layout_toRightOf="@+id/home_manager_get_money"
            android:text="待收佣金："
            android:textColor="#ffffff"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/home_manager_give_money"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/home_manager_give_money_text"
            android:layout_alignBottom="@+id/home_manager_give_money_text"
            android:layout_toRightOf="@+id/home_manager_give_money_text"
            android:text=""
            android:textColor="#ffffff"
            android:textSize="12sp" />

        <com.BeeFramework.view.WebImageView
            android:id="@+id/home_manager_img"
            android:layout_width="54dp"
            android:layout_height="64dp"
            android:layout_marginRight="24dp"
            android:layout_alignParentRight="true"
            android:layout_alignTop="@+id/home_logo"
            android:src="@drawable/icon_default_user_img" />

        <TextView
            android:id="@+id/home_manager_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/home_manager_get_money_text"
            android:layout_below="@+id/home_manager_get_money"
            android:layout_marginTop="8dp"
            android:text="昵称："
            android:textColor="#ffffff"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/home_manager_name_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/home_manager_name"
            android:layout_alignBottom="@+id/home_manager_name"
            android:layout_toRightOf="@+id/home_manager_name"
            android:text=""
            android:textColor="#ffffff"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/home_manager_recommend_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/home_manager_name_text"
            android:layout_alignBottom="@+id/home_manager_name_text"
            android:layout_alignLeft="@+id/home_manager_give_money_text"
            android:text="推荐码："
            android:textColor="#ffffff"
            android:textSize="12sp" 
          
            />

        <TextView
            android:id="@+id/home_manager_recommend"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/home_manager_recommend_text"
            android:layout_alignBottom="@+id/home_manager_recommend_text"
            android:layout_toRightOf="@+id/home_manager_recommend_text"
            android:text=""
            android:textColor="#ffffff"
            android:textSize="12sp" 
           
            />

    </RelativeLayout>

    <LinearLayout
        android:id="@+id/manager_func"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/manager_info"
        android:orientation="horizontal" >

        <LinearLayout
            android:id="@+id/home_manage_client"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8dp"
            android:layout_marginLeft="16dp"
            android:layout_marginRight="8dp"
            android:layout_marginTop="32dp"
            android:layout_weight="1.0"
            android:orientation="vertical" >

            <ImageView
                android:id="@+id/home_manage_client_img"
                android:layout_width="48dp"
                android:layout_height="48dp"
                android:layout_gravity="center"
                
                android:src="@drawable/icon_my_manager"  />

            <TextView
                android:id="@+id/home_manage_client_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginTop="16dp"
                android:text="客户管理"
                android:textColor="#6B6B6B"
                android:textSize="16sp" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/home_manage_house_check"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="24dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"
            android:layout_marginTop="32dp"
            android:layout_weight="1.0"
            android:orientation="vertical" >

            <ImageView
                android:id="@+id/home_manage_house_check_img"
                android:layout_width="48dp"
                android:layout_height="48dp"
                android:layout_gravity="center"
                
                android:src="@drawable/icon_my_house" />

            <TextView
                android:id="@+id/home_manage_house_check_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginTop="16dp"
                android:text="房源查看"
                android:textColor="#6B6B6B"
                android:textSize="16sp" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/home_manage_see_client"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="24dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"
            android:layout_marginTop="32dp"
            android:layout_weight="1.0"
            android:orientation="vertical" >

            <ImageView
                android:id="@+id/home_manage_see_client_img"
                android:layout_width="48dp"
                android:layout_height="48dp"
                android:layout_gravity="center"
                android:src="@drawable/icon_my_wallet"
                />

            <TextView
                android:id="@+id/home_manage_see_client_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginTop="16dp"
                android:text="接待客户"
                android:textColor="#6B6B6B"
                android:textSize="16sp" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/notification_wrapper"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/manager_func"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        android:orientation="horizontal" >

        <ImageView
            android:id="@+id/notification_img"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:layout_marginLeft="24dp"
            android:src="@drawable/icon_notification" />

        <TextView
            android:id="@+id/notification_text"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:background="#ffffff"
            android:paddingLeft="8dp"
            android:text="易屋之家总发放佣金"
             android:layout_marginRight="20dp"
            android:textSize="12sp"
            android:textColor="#777777" />
    </LinearLayout>

    <TextView
        android:id="@+id/home_rank_text"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="24dp"
        android:layout_marginRight="24dp"
        android:padding="8dp"
        android:layout_below="@+id/notification_wrapper"
        android:text="15天排行榜"
        android:textColor="#FE9929" />

    <LinearLayout 
        android:id="@+id/rank_list_wrapper"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_below="@+id/home_rank_text"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:background="@drawable/grid_shape"
        >
        <GridView
	        android:id="@+id/rank_list"
	        android:layout_width="fill_parent"
	        android:layout_height="150dp"
	        android:clickable="false"
	        android:cacheColorHint="#00000000"
	        android:scrollbars="none" 
	        android:numColumns="1" 
	        
	         >
	    </GridView>
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="24dp"
        android:layout_marginRight="24dp"
        android:layout_marginBottom="8dp"
        android:layout_alignParentBottom="true"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/change_psd"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:layout_weight="1.0"
            android:background="#ffffff"
            android:gravity="center"
            android:paddingBottom="6dp"
            android:text="修改密码"
            android:textColor="#6F6F6F"
            android:textSize="16sp" />

        <TextView
            android:id="@+id/login_out_text"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:layout_weight="1.0"
            android:background="#ffffff"
            android:gravity="center"
            android:paddingBottom="6dp"
            android:text="退出登录"
            android:textColor="#6F6F6F"
            android:textSize="16sp" />
    </LinearLayout>

</RelativeLayout>
